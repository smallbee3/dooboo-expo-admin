{
  "name": "dooboo-expo-admin",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "codecov": "codecov",
    "tsc": "tsc",
    "start": "yarn relay && expo start",
    "cp:config": "cp config.sample.ts config.ts",
    "production": "yarn relay && expo start --no-dev",
    "android": "yarn relay && expo start --android",
    "ios": "yarn relay && expo start --ios",
    "eject": "expo eject",
    "jest": "node node_modules/jest/bin/jest.js",
    "test": "yarn lint && yarn tsc && node node_modules/jest/bin/jest.js",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "relay": "yarn run relay-compiler --schema src/relay/schema.graphql --src ./src/ --extensions ts tsx --language typescript --artifactDirectory ./src/__generated__ --watchman false $@",
    "relay-watch": "yarn run relay-compiler --schema src/relay/schema.graphql --src ./src/ --extensions ts tsx --language typescript --artifactDirectory ./src/__generated__ --watch",
    "schema": "get-graphql-schema https://primedev.azurewebsites.net/graphql > src/relay/schema.graphql",
    "schema:local": "get-graphql-schema http://localhost:5000/graphql > src/relay/schema.graphql",
    "codegen": "graphql-codegen --config codegen.yml",
    "generate": "yarn schema && yarn codegen"
  },
  "private": true,
  "version": "0.0.0",
  "description": "Expo starter from dooboo-cli.",
  "author": "Clark <clark@dooboolab.com>",
  "license": "MIT",
  "dependencies": {
    "@dooboo-ui/theme": "^0.0.1",
    "@expo/match-media": "^0.0.0-beta.2",
    "@expo/webpack-config": "^0.12.20",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/core": "^5.12.1",
    "@react-navigation/native": "^5.7.1",
    "@react-navigation/stack": "^5.7.1",
    "dooboo-ui": "^0.0.2",
    "expo": "^38.0.8",
    "expo-auth-session": "^1.4.1",
    "expo-linear-gradient": "^8.2.1",
    "expo-localization": "~8.2.1",
    "expo-splash-screen": "^0.4.0",
    "i18n-js": "^3.7.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.0.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "3.0.2",
    "react-native-screens": "~2.9.0",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.13.3",
    "react-native-webview": "9.4.0",
    "react-navigation-stack": "^2.3.10",
    "react-relay": "^10.0.0",
    "react-responsive": "^8.1.0",
    "sentry-expo": "^2.1.2",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-typescript": "^7.10.4",
    "@dooboo/eslint-config": "^0.4.3",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/core": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-native": "^3.1.0",
    "@testing-library/react-native": "^5.0.3",
    "@types/expo": "^33.0.1",
    "@types/i18n-js": "^3.0.3",
    "@types/jest": "^26.0.5",
    "@types/react": "~16.9.43",
    "@types/react-native": "~0.63.1",
    "@types/react-relay": "^7.0.8",
    "@types/react-responsive": "^8.0.2",
    "@types/react-test-renderer": "^16.9",
    "@types/relay-runtime": "^10.0.1",
    "@types/relay-test-utils": "^6.0.2",
    "@types/styled-components": "^5.1.1",
    "babel-jest": "^26.1.0",
    "babel-plugin-relay": "^10.0.0",
    "babel-preset-expo": "^8.2.3",
    "babel-preset-react-native": "4.0.1",
    "codecov": "^3.7.1",
    "eslint": "^7.5.0",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^15.3.0",
    "jest": "^26.1.0",
    "jest-expo": "^38.0.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^7.0.2",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.60.0",
    "prettier": "^2.0.5",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "^16.13.1",
    "relay-compiler": "^10.0.0",
    "relay-compiler-language-typescript": "^12.0.3",
    "relay-config": "^10.0.0",
    "relay-test-utils": "^10.0.0",
    "ts-jest": "^26.1.3",
    "tslib": "^2.0.0",
    "typescript": "~3.9.7"
  }
}
