{
  "name": "prime",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "codecov": "codecov",
    "tsc": "tsc",
    "start": "yarn relay && expo start",
    "cp:config": "cp config.sample.ts config.ts",
    "production": "yarn relay && expo start --no-dev",
    "android": "yarn relay && expo start --android",
    "ios": "yarn relay && expo start --ios",
    "eject": "expo eject",
    "jest": "node node_modules/jest/bin/jest.js",
    "test": "yarn lint && yarn tsc && node node_modules/jest/bin/jest.js",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "relay": "yarn run relay-compiler --schema src/relay/schema.graphql --src ./src/ --extensions ts tsx --language typescript --artifactDirectory ./src/__generated__ --watchman false $@",
    "relay-watch": "yarn run relay-compiler --schema src/relay/schema.graphql --src ./src/ --extensions ts tsx --language typescript --artifactDirectory ./src/__generated__ --watch",
    "schema": "get-graphql-schema https://primedev.azurewebsites.net/graphql > src/relay/schema.graphql",
    "codegen": "graphql-codegen --config codegen.yml",
    "generate": "yarn schema && yarn codegen"
  },
  "private": true,
  "version": "0.0.0",
  "description": "Expo starter from dooboo-cli.",
  "author": "hyochan <dooboolab@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@dooboo-ui/theme": "^0.0.1",
    "@expo/match-media": "^0.0.0-alpha.5",
    "@expo/webpack-config": "^0.12.18",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/core": "^5.11.1",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "dooboo-ui": "^0.0.3",
    "expo": "^38.0.3",
    "expo-auth-session": "~1.4.0",
    "expo-linear-gradient": "~8.2.1",
    "expo-localization": "~8.2.1",
    "expo-splash-screen": "^0.3.1",
    "i18n-js": "^3.7.0",
    "react": "0.0.0-experimental-f42431abe",
    "react-dom": "0.0.0-experimental-f42431abe",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.0.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.12.3",
    "react-native-webview": "9.4.0",
    "react-navigation-stack": "^2.3.10",
    "react-relay": "^0.0.0-experimental-94e87455",
    "react-responsive": "^8.1.0",
    "sentry-expo": "^2.1.2",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@dooboo/eslint-config": "^0.4.2",
    "@graphql-codegen/cli": "^1.16.3",
    "@graphql-codegen/core": "^1.16.3",
    "@graphql-codegen/typescript": "^1.16.3",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-native": "^3.1.0",
    "@testing-library/react-native": "^5.0.3",
    "@types/expo": "^33.0.1",
    "@types/i18n-js": "^3.0.3",
    "@types/jest": "^26.0.3",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "@types/react-relay": "^7.0.8",
    "@types/react-responsive": "^8.0.2",
    "@types/react-test-renderer": "^16.9",
    "@types/relay-runtime": "^9.1.5",
    "@types/relay-test-utils": "^6.0.2",
    "@types/styled-components": "^5.1.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-relay": "^9.1.0",
    "babel-preset-expo": "^8.2.3",
    "babel-preset-react-native": "4.0.1",
    "codecov": "^3.7.0",
    "eslint": "^7.3.1",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^15.3.0",
    "jest": "^26.1.0",
    "jest-expo": "^38.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^7.0.2",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.5",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "^16.13.1",
    "relay-compiler": "^9.1.0",
    "relay-compiler-language-typescript": "^12.0.3",
    "relay-config": "^9.1.0",
    "relay-test-utils": "^9.1.0",
